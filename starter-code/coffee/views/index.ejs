<!-- main.html -->
<!DOCTYPE html>
<html>
  <head>
    <title>My First Map</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <link rel='stylesheet' href='/stylesheets/style.css'>
  </head>
  <body>
    <h3>My First Map</h3>
    <form method="POST" action="/locations" id="new-location-form" class="location-form">
      <input type="text" name="name"        placeholder="name">
      <input type="text" name="description" placeholder="description">

      <label>Address</label>
      <input type="text" name="latitude" placeholder="Latitude" >
      <input type="text" name="longitude" placeholder="Longitude">

      <input type="submit" name="" id="send-data">
    </form>
    <div id="map"></div>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBQ5BCn7TRXifLLeL8yz_g8ko-YbwimNmk"></script>
    <script type="text/javascript" src="javascripts/main.js"></script>

  </body>
</html>
